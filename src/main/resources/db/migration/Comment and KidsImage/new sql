ALTER TABLE UserImageLikes
    ADD CONSTRAINT fk_UserImageLikes_UserLogin
        FOREIGN KEY (user_id) REFERENCES UserLogin(user_id);

CREATE TABLE UserImageLikes (
PRIMARY Key(user_id,img_id)
                                user_id INT,
                                img_id INT,
                                liked_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
                                PRIMARY KEY (user_id, img_id)

);
ALTER TABLE Comment
    ADD COLUMN hasLiked BOOLEAN DEFAULT FALSE;
ALTER TABLE Comment
    ADD COLUMN likes int DEFAULT 0;
