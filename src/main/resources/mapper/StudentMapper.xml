<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soholighting.sohoTeam8.cxMapper.KidsImageMapper">
    <select id="findAll" resultType="com.soholighting.sohoTeam8.cxEnity.KidsImage">
        SELECT * FROM KidsImge
    </select>
    <select id="findAllByYear" parameterType="java.lang.String" resultType="com.soholighting.sohoTeam8.cxEnity.KidsImage">
        SELECT * FROM KidsImge WHERE issueDate LIKE CONCAT(#{year}, '%')
    </select>
</mapper>